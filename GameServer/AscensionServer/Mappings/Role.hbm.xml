<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="AscensionServer"
                   namespace="AscensionServer.Model">
  
  <class name="Role" table="role_stats">
    <id name="RoleId" column="role_id" type="Int32">
      <generator  class="native"/>
    </id>
    <property name="RoleName" column="role_name" type="String"/>
    <property name="RoleRoot" column="role_root" type="String"/>
    <property name="RoleGender" column="role_gender" type="Byte"/>
    <property name="RoleHP" column="role_hp" type="int"/>
    <property name="RoleMP" column="role_mp" type="int"/>
    <property name="RoleJingXue" column="role_jingxue" type="Byte"/>
    <property name="RoleAttackDamage" column="role_attack_damage" type="int"/>
    <property name="RoleResistanceDamage" column="role_resistance_damage" type="int"/>
    <property name="RoleAttackPower" column="role_attack_power" type="int"/>
    <property name="RoleResistancePower" column="role_resistance_power" type="int"/>
    <property name="RoleSpeedAttack" column="role_speed_attack" type="int"/>
    <property name="RoleShenHunDamage" column="role_shenhun_damage" type="int"/>
    <property name="RoleShenHunResistance" column="role_shenhun_resistance" type="int"/>
    <property name="RoleCrit" column="role_crit" type="Byte"/>
    <property name="RoleCritResistance" column="role_crit_resistance" type="Byte"/>
    <property name="RoleUserAccount" column="role_user_account" type="String"/>
    <many-to-one name="_User" column="account" not-null="true" class="User" foreign-key="fk_role_user_account" />
    <set name="RoleGongFaList" table="role_gongfa" generic="true" inverse="true">
      <key column="role_id" foreign-key="fk_role_id"/>
      <one-to-many class="RoleGongFa"/>
    </set>
    <set name="RoleLevelList" table="role_level" generic="true" inverse="true">
      <key column="role_id" foreign-key="fk_role_id"/>
      <one-to-many class="RoleLevel"/>
    </set>
    
  </class>
</hibernate-mapping>