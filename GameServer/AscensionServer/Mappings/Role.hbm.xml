<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="AscensionServer"
                   namespace="AscensionServer.Model">
  
  <class name="Role" table="role">
    <id name="RoleId" column="role_id" type="Int32">
      <generator  class="native"/>
    </id>
    <property name="RoleName" column="role_name" type="String"/>
    <property name="RoleFaction" column="role_faction" type="Byte"/>
    <property name="RoleRoot" column="role_root" type="String"/>
    <property name="RoleExp" column="role_exp" type="Int32"/>
    <property name="RoleLevel" column="role_level" type="Int32"/>
    <property name="RoleGender" column="role_gender" type="Byte"/>
   
  </class>
</hibernate-mapping>