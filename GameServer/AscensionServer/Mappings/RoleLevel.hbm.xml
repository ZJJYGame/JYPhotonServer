<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="AscensionServer"
                   namespace="AscensionServer.Model">

  <class name="RoleLevel" table="role_level">
    <id name="Id" column="id" type="Int32">
      <generator  class="native"/>
    </id>
    <property name="RoleId" column="role_id" type="int"/>
    <property name="RoleCurrentLevel" column="role_current_level" type="Byte"/>
    <property name="RoleExp" column="role_exp" type="int"/>
  </class>
</hibernate-mapping>