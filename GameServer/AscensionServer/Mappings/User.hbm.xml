<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="AscensionServer"
                   namespace="AscensionServer.Model">

  <class name="User" table="user">
    <id name="UUID" column="uuid" type="String">
      <generator class="uuid.hex" />
    </id>
    <property name="Account" column="account" type="String" />
    <property name="Password" column="password" type="String" />
    <set name ="UserroleList" table = "user_role" generic = "true" inverse = "true">
      <key column ="uuid" foreign-key ="fk_ur_uuid"/>
      <one-to-many class  = "UserRole"/>
    </set>
  </class>
</hibernate-mapping>

